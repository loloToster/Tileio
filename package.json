{
  "name": "widgetblocks",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "watch-ts": "webpack --config ./src/public/webpack.dev.js",
    "watch-sass": "sass -w ./src/public/src/sass/:./src/public/static/css ./src/public/src/dynamic-cells/:./src/public/static/dynamic-cells --no-source-map",
    "build-client": "webpack --config ./src/public/webpack.prod.js",
    "build-server-batch": "tsc && xcopy .\\src\\public\\views\\ .\\dist\\public\\views\\ /Y /E && xcopy .\\src\\public\\static\\ .\\dist\\public\\static\\ /Y /E",
    "build-server-bash": "tsc && mkdir -p ./dist/public && cp -r ./src/public/views ./dist/public/views && cp -r ./src/public/static ./dist/public/static",
    "build-batch": "npm run build-client && npm run build-server-batch",
    "build": "npm run build-client && npm run build-server-bash",
    "start": "node ./dist/app.js",
    "test": "ts-node ./src/app.ts",
    "start-service": "qckwinsvc --name WidgetBlocks --description WidgetBlocks --script ./dist/app.js --startImmediately",
    "stop-service": "qckwinsvc --uninstall --name WidgetBlocks --script ./dist/app.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cookie-session": "^2.0.44",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/passport": "^1.0.8",
    "@types/passport-discord": "^0.1.5",
    "@types/passport-github2": "^1.2.5",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/spotify-web-api-node": "^5.0.7",
    "@types/spotify-web-playback-sdk": "^0.1.12",
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.12",
    "cookie-session": "^2.0.0",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "frameguard": "^4.0.0",
    "fuse.js": "^6.6.2",
    "gridstack": "^5.1.1",
    "heroku-ssl-redirect": "^0.1.1",
    "kalender-events": "^0.28.1",
    "marked": "^4.0.18",
    "mathjs": "^11.0.1",
    "mongoose": "^6.3.5",
    "openweather-api-node": "^1.4.1",
