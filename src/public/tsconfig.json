{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@backend-types/*": [
        "../types/*"
      ]
    },
    "rootDirs": [
      "./src",
      "../types"
    ],
    "target": "es5",
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "./static",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["DOM"]
  }
}
