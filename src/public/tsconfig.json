{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@backend-types/*": [
        "../types/*"
      ]
    },
    "rootDirs": [
      "./src/ts",
      "../types"
    ],
    "target": "es5",
    "module": "es6",
    "moduleResolution": "node",
    "outDir": "./static/js",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
